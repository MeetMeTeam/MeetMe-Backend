    "definitions": {
        "interfaces.InviteRequest": {
            "type": "object",
            "properties": {
                "targetMailAddress": {
                    "type": "string",
                    "example": "winner@mail.com"
                }
            }
        },
        "interfaces.Login": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "example": "winner@mail.com"
                },
                "password": {
                    "type": "string",
                    "example": "winner"
                }
            }
        },
        "interfaces.RegisterRequest": {
            "type": "object",
            "required": [
                "email",
                "firstname",
                "image",
                "password"
            ],
            "properties": {
                "birthday": {
                    "type": "string",
                    "example": "2023-08-12"
                },
                "email": {
                    "type": "string",
                    "example": "winner@mail.com"
                },
                "firstname": {
                    "type": "string",
                    "example": "Kanyapat"
                },
                "image": {
                    "type": "string"
                },
                "lastname": {
                    "type": "string",
                    "example": "Wittayamitkul"
                },
                "password": {
                    "type": "string",
                    "example": "winner"
                },
                "username": {
                    "type": "string",
                    "example": "winnerkypt"
                }
            }
        },
        "utils.DataResponse": {
            "type": "object",
            "properties": {
                "data": {},
                "message": {
                    "type": "string"
                }
            }
        }
